{
  "name": "mill",
  "version": "0.0.1",
  "description": "Yet another blog system.",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "./node_modules/.bin/webpack --progress --colors",
    "watch": "npm run build && node server.js",
    "predist": "npm run clean",
    "dist": "webpack --progress --colors --config webpack.config.production.js",
    "predeploy": "npm run dist",
    "deploy": "./node_modules/.bin/gh-pages --dist dist --src !*.map",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "history": "~1.13.0",
    "qs": "~6.0.1",
    "react": "~0.14.3",
    "react-dom": "~0.14.3",
    "react-router": "~1.0.2",
    "whatwg-fetch": "~0.10.1"
  },
  "devDependencies": {
    "babel-core": "~6.3.21",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "copy-webpack-plugin": "^0.3.3",
    "exports-loader": "^0.6.2",
    "gh-pages": "^0.8.0",
    "html-webpack-plugin": "^1.7.0",
    "imports-loader": "^0.6.5",
    "webpack": "~1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "homepage": "https://github.com/rockdai/mill",
  "repository": {
    "type": "git",
    "url": "git@github.com:rockdai/mill.git",
    "web": "https://github.com/rockdai/mill"
  },
  "bugs": {
    "url": "https://github.com/rockdai/mill/issues",
    "email": "rockdai321@gmail.com"
  },
  "license": "MIT"
}
